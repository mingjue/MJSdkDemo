<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dataenlighten.aimjsdk.demo">

    <!-- 网络状态 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 获取手机录音机使用权限，听写、识别、语义V理解需要用到此权限 -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <!-- 获取网络状态，请求网络前判断是否有网络 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DrawActivity"
            android:label="@string/draw" />
        <activity
            android:name=".VinQueryActivity"
            android:label="@string/vinQurey" />
        <activity
            android:name=".PartListActivity"
            android:label="@string/partList" />
        <activity
            android:name=".RelatedPartActivity"
            android:label="@string/relatedPart" />
        <activity
            android:name=".AnalysisActivity"
            android:label="@string/AnalysisPart" />


    </application>

</manifest>